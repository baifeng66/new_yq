<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<log4net>
		<!--信息日志-->
		<appender name="InfoLoging" type="log4net.Appender.RollingFileAppender,log4net" >
			<param name="File" value="Log\" />
			<param name="AppendToFile" value="true" />
			<!--日志滚动形式：根据日期滚动-->
			<param name="RollingStyle" value="Date" />
			<!--日志文件格式-->
			<param name="DatePattern" value="yyyyMM\\yyyyMMdd&quot;info.log&quot;" />
			<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
			<!--最大文件大小-->
			<param name="maximumFileSize" value="5MB"/>
			<!--日志文件名是否是固定不变的-->
			<param name="StaticLogFileName" value="false" />
			<!--最多保留的日志文件数量-->
			<param name="MaxSizeRollBackups" value="7" />
			<layout type="log4net.Layout.PatternLayout,log4net">
				<!--<param name="ConversionPattern" value="%-5p %d [%c] %m%n"  />-->
				<param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<param name="LevelMin" value="INFO" />
				<param name="LevelMax" value="INFO" />
			</filter>
		</appender>
		<!--警告日志-->
		<appender name="WarnLoging" type="log4net.Appender.RollingFileAppender,log4net" >
			<param name="File" value="Log\" />
			<param name="AppendToFile" value="true" />
			<param name="RollingStyle" value="Date" />
			<!--日志文件格式-->
			<param name="DatePattern" value="yyyyMM\\yyyyMMdd&quot;warn.log&quot;" />
			<param name="StaticLogFileName" value="false" />
			<!--最多保留的日志文件数量-->
			<param name="MaxSizeRollBackups" value="7" />
			<layout type="log4net.Layout.PatternLayout,log4net">
				<!--<param name="ConversionPattern" value="%-5p %d [%c] %m%n"  />-->
				<param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<param name="LevelMin" value="WARN" />
				<param name="LevelMax" value="WARN" />
			</filter>
		</appender>

		<!--调试日志-->
		<appender name="EventLoging" type="log4net.Appender.RollingFileAppender,log4net" >
			<param name="File" value="Log\" />
			<param name="AppendToFile" value="true" />
			<param name="RollingStyle" value="Date" />
			<!--日志文件格式-->
			<param name="DatePattern" value="yyyyMM\\yyyyMMdd&quot;debug.log&quot;" />
			<param name="StaticLogFileName" value="false" />
			<!--最多保留的日志文件数量-->
			<param name="MaxSizeRollBackups" value="7" />
			<layout type="log4net.Layout.PatternLayout,log4net">
				<!--<param name="ConversionPattern" value="%-5p %d [%c] %m%n"  />-->
				<param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<param name="LevelMin" value="DEBUG" />
				<param name="LevelMax" value="DEBUG" />
			</filter>
		</appender>
		<!--错误日志-->
		<appender name="ErrorLoging" type="log4net.Appender.RollingFileAppender,log4net" >
			<param name="File" value="Log\" />
			<param name="AppendToFile" value="true" />
			<param name="RollingStyle" value="Date" />
			<!--日志文件格式-->
			<param name="DatePattern" value="yyyyMM\\yyyyMMdd&quot;error.log&quot;" />
			<param name="StaticLogFileName" value="false" />
			<!--最多保留的日志文件数量-->
			<param name="MaxSizeRollBackups" value="30" />
			<layout type="log4net.Layout.PatternLayout,log4net">
				<!--<param name="ConversionPattern" value="%-5p %d [%c] %m%n"  />-->
				<param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<param name="LevelMin" value="ERROR" />
				<param name="LevelMax" value="ERROR" />
			</filter>
		</appender>
		<!--奔溃日志-->
		<appender name="FatalLoging" type="log4net.Appender.RollingFileAppender,log4net" >
			<param name="File" value="Log\" />
			<param name="AppendToFile" value="true" />
			<param name="RollingStyle" value="Date" />
			<!--日志文件格式-->
			<param name="DatePattern" value="yyyyMM\\yyyyMMdd&quot;fatal.log&quot;" />
			<param name="StaticLogFileName" value="false" />
			<!--最多保留的日志文件数量-->
			<param name="MaxSizeRollBackups" value="30" />
			<layout type="log4net.Layout.PatternLayout,log4net">
				<param name="ConversionPattern" value="%d [%t] %-5p %c - %m%n" />
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<param name="LevelMin" value="Fatal" />
				<param name="LevelMax" value="Fatal" />
			</filter>
		</appender>
		<!-- 控制台前台显示日志 -->
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%n%date{HH:mm:ss,fff} [%t] [%-5level] %m" />
			</layout>
			<filter type="log4net.Filter.LevelRangeFilter">
				<param name="LevelMin" value="Info" />
				<param name="LevelMax" value="Fatal" />
			</filter>
		</appender>
		<logger name="AppLog">
			<level value="ALL" />
			<appender-ref ref="InfoLoging" />
			<appender-ref ref="EventLoging"/>
			<appender-ref ref="WarnLoging"/>
			<appender-ref ref="ErrorLoging" />
			<appender-ref ref="FatalLoging" />
			<appender-ref ref="ConsoleAppender" />
		</logger>
	</log4net>
</configuration>